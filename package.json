{
	"name": "leondeklerk",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git://github.com/leondeklerk/leondeklerk"
	},
	"scripts": {
		"dev": "bunx --bun vite",
		"build": "bun run lint && bun run type-check && bun run build-only",
		"build-only": "bunx --bun vite build",
		"lint": "eslint --ext .ts,.vue,.js --max-warnings 0 .",
		"lint:fix": "eslint --ext .ts,.vue,.js --fix .",
		"prepare": "husky",
		"preview": "bunx --bun vite preview",
		"test:unit": "bunx --bun vitest",
		"type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false "
	},
	"lint-staged": {
		"*.{js,ts,vue}": "eslint --ext .ts,.vue,.js --max-warnings 0"
	},
	"dependencies": {
		"@fortawesome/fontawesome-free": "6.5.2",
		"vue": "3.4.26"
	},
	"devDependencies": {
		"@rushstack/eslint-patch": "1.10.2",
		"@tsconfig/node20": "20.1.4",
		"@types/bun": "1.1.1",
		"@vitejs/plugin-vue": "5.0.4",
		"@vue/eslint-config-prettier": "9.0.0",
		"@vue/eslint-config-typescript": "13.0.0",
		"@vue/tsconfig": "0.5.1",
		"autoprefixer": "10.4.19",
		"eslint": "8.57.0",
		"eslint-plugin-vue": "9.25.0",
		"husky": "9.0.11",
		"lint-staged": "15.2.2",
		"postcss": "8.4.38",
		"prettier": "3.2.5",
		"tailwindcss": "3.4.3",
		"typescript": "5.4.5",
		"vite": "5.2.11",
		"vue-tsc": "2.0.16"
	}
}
